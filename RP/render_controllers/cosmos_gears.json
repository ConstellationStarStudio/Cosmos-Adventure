{
  "format_version": "1.20.80",
  "render_controllers": {
    "controller.render.cosmos_gears": {
      "geometry": "geometry.cosmos_gears",
      "materials": [
        {
          "*": "material.default"
        }
      ],
      "part_visibility": [
        {
          "gear": "query.property('cosmos:oxygen_gear')"
        },
        {
          "mask": "query.property('cosmos:oxygen_mask')"
        },
        {
          "tank_11": "query.property('cosmos:tank1') == 'light'"
        },
        {
          "tank_12": "query.property('cosmos:tank1') == 'medium'"
        },
        {
          "tank_13": "query.property('cosmos:tank1') == 'heavy'"
        },
        {
          "tank_21": "query.property('cosmos:tank2') == 'light'"
        },
        {
          "tank_22": "query.property('cosmos:tank2') == 'medium'"
        },
        {
          "tank_23": "query.property('cosmos:tank2') == 'heavy'"
        },
        {
          "frequency": "query.property('cosmos:frequency_module')"
        },
        {
          "frequency_radar": "query.property('cosmos:frequency_module')"
        }
      ],
      "textures": [
        "texture.cosmos_gears"
      ]
    }
  }
}
{
  "format_version": "1.20.80",
  "render_controllers": {
    "controller.render.cosmos_gears": {
      "geometry": "geometry.cosmos_gears",
      "materials": [
        {
          "*": "material.default"
        }
      ],
      "part_visibility": [
        {
          "gear": "!variable.is_paperdoll && query.property('cosmos:oxygen_gear')"
        },
        {
          "mask": "!variable.is_paperdoll && query.property('cosmos:oxygen_mask')"
        },
        {
          "tank_11": "!variable.is_paperdoll && query.property('cosmos:tank1') == 'light'"
        },
        {
          "tank_12": "!variable.is_paperdoll && query.property('cosmos:tank1') == 'medium'"
        },
        {
          "tank_13": "!variable.is_paperdoll && query.property('cosmos:tank1') == 'heavy'"
        },
        {
          "tank_21": "!variable.is_paperdoll && query.property('cosmos:tank2') == 'light'"
        },
        {
          "tank_22": "!variable.is_paperdoll && query.property('cosmos:tank2') == 'medium'"
        },
        {
          "tank_23": "!variable.is_paperdoll && query.property('cosmos:tank2') == 'heavy'"
        },
        {
          "frequency": "!variable.is_paperdoll && query.property('cosmos:frequency_module')"
        },
        {
          "frequency_radar": "!variable.is_paperdoll && query.property('cosmos:frequency_module')"
        }
      ],
      "textures": [
        "texture.cosmos_gears"
      ]
    }
  }
}

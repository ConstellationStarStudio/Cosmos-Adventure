            "sequence": [
               {
                  "add": {
                     "component_groups": [
                        "baby"
                     ]
                  }
               },
               {
                  "add": {
                     "component_groups": [
                        "farmer",
                        "behavior_peasant"
                     ]
                  }
               }
            ]
         },
         "minecraft:ageable_grow_up": {
            "sequence": [
               {
                  "add": {
                     "component_groups": [
                        "adult"
                     ]
                  },
                  "remove": {
                     "component_groups": [
                        "baby"
                     ]
                  }
               }
            ]
         },
         "become_normal_villager": {
            "add": {
               "component_groups": [

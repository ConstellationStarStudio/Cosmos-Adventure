      "events": {
         "minecraft:entity_transformed": {
            "sequence": [
               {
                  "filters": {
                     "subject": "other",
                     "value": "minecraft:is_baby",
                     "test": "has_component"
                  },
                  "add": {
                     "component_groups": [
                        "baby"
                     ]
                  }
               },
               {
                  "filters": {
                     "test": "has_component",
                     "subject": "other",
                     "value": "minecraft:is_baby",
                     "operator": "!="
                  },
                  "add": {
                     "component_groups": [
                        "adult"
                     ]
                  }
               },
               {
                  "filters": {
                     "subject": "other",
                     "value": "farmer",
                     "test": "is_family"
                  },
                  "add": {
                     "component_groups": [
                        "farmer",
                        "behavior_peasant"
                     ]
                  }
               }
            ]
         },
         "minecraft:entity_spawned": {
            "sequence": [
               {
                  "filters": {
                     "operator": "!=",
                     "value": "minecraft:variant",
                     "test": "has_component"
                  },
                  "randomize": [
                     {
                        "add": {
                           "component_groups": [
                              "baby"
                           ]
                        },
                        "weight": 5
                     },
                     {
                        "add": {
                           "component_groups": [
                              "adult"
                           ]
                        },
                        "weight": 95
                     }
                  ]
               },
               {
                  "filters": {
                     "operator": "!=",
                     "value": "minecraft:variant",
                     "test": "has_component"
                  },

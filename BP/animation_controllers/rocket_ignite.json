{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.rocket_ignite": {
			"initial_state": "loop",
			"states": {
				"loop": {
					"transitions": [
						{
							"ignited": "1.0"
						}
					]
				},
				"ignited": {
					"transitions": [
						{
							"loop": "1.0"
						}
					],
					"on_exit": [
						"/scriptevent cosmos:rocket ignite"
					]
				}
			}
		}
	}
}